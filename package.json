{
    "name": "laravel-goto-env",
    "displayName": "Laravel Goto Env",
    "description": "Quick jump to env",
    "version": "0.3.1",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.68.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-env",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onUri",
        "workspaceContains:artisan",
        "onLanguage:php",
        "onLanguage:blade",
        "onStartupFinished"
    ],
    "main": "./out/main",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Env",
            "properties": {
                "laravelGotoEnv.methods": {
                    "type": "array",
                    "default": [
                        "env",
                        "Env::get",
                        "Env::set"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                },
                "laravelGotoEnv.envFiles": {
                    "type": "array",
                    "default": [
                        ".env"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "env files to read from"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run esbuild-base -- --minify",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
        "watch": "npm run vscode:prepublish",
        "test-compile": "tsc -p ./"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/node": "^18.11.9",
        "@types/vscode": "^1.68.0",
        "esbuild": "^0.15.13",
        "glob": "^8.0.3",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "escape-string-regexp": "^5.0.0",
        "lodash": "^4.17.21"
    }
}
