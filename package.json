{
    "name": "laravel-goto-env",
    "displayName": "Laravel Goto Env",
    "description": "Quick jump to env",
    "version": "0.0.9",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-env",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onUri",
        "workspaceContains:artisan"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Env",
            "properties": {
                "laravel_goto_env.methods": {
                    "type": "array",
                    "default": [
                        "env",
                        "Env::get",
                        "Env::set"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.3",
        "@types/node": "^14.6.0",
        "@types/vscode": "^1.48.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "escape-string-regexp": "^4.0.0",
        "lodash.debounce": "^4.0.8"
    }
}
