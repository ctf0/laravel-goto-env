{
    "name": "laravel-goto-env",
    "displayName": "Laravel Goto Env",
    "description": "Quick jump to env",
    "version": "0.2.3",
    "publisher": "ctf0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "repository": "https://github.com/ctf0/laravel-goto-env",
    "icon": "images/logo.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel",
        "goto",
        "file navigation"
    ],
    "activationEvents": [
        "onUri",
        "workspaceContains:artisan",
        "onLanguage:php",
        "onLanguage:blade",
        "onStartupFinished"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Laravel Goto Env",
            "properties": {
                "laravelGotoEnv.methods": {
                    "type": "array",
                    "default": [
                        "env",
                        "Env::get",
                        "Env::set"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "methods to search for"
                },
                "laravelGotoEnv.envFiles": {
                    "type": "array",
                    "default": [
                        ".env"
                    ],
                    "items": {
                        "type": "string"
                    },
                    "minItems": 1,
                    "uniqueItems": true,
                    "description": "env files to read from"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.21",
        "@types/vscode": "^1.51.0",
        "@vscode/test-electron": "^2.1.3",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "escape-string-regexp": "^4.0.0",
        "lodash": "^4.17.21"
    }
}
